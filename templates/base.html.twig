        {% block javascripts %}{% endblock %}
        {% block javascriptsApp %}
            <script>
                const cookieAccept = document.querySelector('.acceptCookie')
                const cookie = document.querySelector('.cookie')
                if(localStorage.getItem('cookie')){
                    cookie.classList.add('cookieAccepted')
                }
                cookieAccept.addEventListener('click',()=>{
                    localStorage.setItem('cookie','QTCookieAccept√©')
                    cookie.classList.add('cookieAccepted')
                })
           </script>
        {% endblock %}
