{% extends "admin/base.html.twig" %}

{% block title %}Ajout Entrainement{% endblock %}

{% block body %}
    <div class="slideAdmin">
        <div class="wrapper">
            <div class="form">
                <div class="titleBack">
                    <h2>Ajout d'un entrainement</h2>
                    <a href="{{path('admin_entrainement_index')}}" class="btn btn-secondary">Retour</a>
                </div>
                {{form_start(formEntrainement)}}
                    {{form_widget(formEntrainement)}}
                    <button type="submit" class="btn btn-primary my-3">Ajouter</button>
                {{form_end(formEntrainement)}}
            </div>
        </div>
    </div>
{% endblock %}